<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="me.lxl.expresstrack.zxing"
    android:versionCode="198"
    android:versionName="1.9.8" >

    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="25" />

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />

    <meta-data
        android:name="android.support.VERSION"
        android:value="25.3.1" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" >
        <activity
            android:name="me.lxl.expresstrack.zxing.LoginActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="me.lxl.expresstrack.zxing.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme" >
        </activity>
        <activity
            android:name="me.lxl.expresstrack.zxing.FullScannerActivity"
            android:label="@string/scanner_activity"
            android:theme="@style/AppOverlayTheme"
            android:uiOptions="splitActionBarWhenNarrow" >
            <meta-data
                android:name="android.support.UI_OPTIONS"
                android:value="splitActionBarWhenNarrow" />
        </activity>
        <activity
            android:name="me.lxl.expresstrack.zxing.FullScannerFragmentActivity"
            android:label="@string/scanner_fragment_activity"
            android:theme="@style/AppOverlayTheme"
            android:uiOptions="splitActionBarWhenNarrow" >
            <meta-data
                android:name="android.support.UI_OPTIONS"
                android:value="splitActionBarWhenNarrow" />
        </activity>
        <activity
            android:name="me.lxl.expresstrack.zxing.SimpleScannerActivity"
            android:label="@string/simple_scanner_activity"
            android:theme="@style/AppOverlayTheme" >
        </activity>
        <activity
            android:name="me.lxl.expresstrack.zxing.SimpleScannerFragmentActivity"
            android:label="@string/simple_scanner_fragment_activity"
            android:theme="@style/AppOverlayTheme" >
        </activity>
        <activity
            android:name="me.lxl.expresstrack.zxing.FullScreenScannerFragmentActivity"
            android:label="@string/full_screen_scanner_fragment_activity"
            android:theme="@style/AppOverlayTheme" >
        </activity>
        <activity
            android:name="me.lxl.expresstrack.zxing.CustomViewFinderScannerActivity"
            android:label="@string/custom_view_finder_scanner_activity"
            android:theme="@style/AppTheme" >
        </activity>
        <activity
            android:name="me.lxl.expresstrack.zxing.ScalingScannerActivity"
            android:label="@string/scan_barcode_button"
            android:theme="@style/AppTheme" >
        </activity>
        <activity
            android:name="me.lxl.expresstrack.zxing.AddNewExpressActivity"
            android:label="@string/add_new_express"
            android:theme="@style/AppTheme" >
        </activity>
    </application>

</manifest>